{
  "name": "freelanc-site",
  "description": "website for getting freelance clients",
  "scripts": {
    "start": "concurrently \"npm:build\" \"npm:start-server\" ",
    "build": "webpack -w",
    "build:prod": " webpack",
    "start-server": "nodemon NODE_ENV='development'  dist/server.js",
    "start-server:prod": "NODE_ENV=production pm2 start dist/server.js --name Freelance-Site"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "concurrently": "^7.2.0",
    "css-loader": "^5.2.6",
    "dotenv-webpack": "^7.1.0",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.0.0",
    "nodemon": "^2.0.16",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.7.2",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "history": "^5.0.0",
    "isomorphic-fetch": "^3.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.0-beta.0",
    "serialize-javascript": "^6.0.0"
  },
  "version": "1.0.0",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uidotdev/react-router-server-rendering.git"
  },
  "author": "Tyler McGinnis",
  "license": "MIT",
  "homepage": "https://github.com/uidotdev/react-router-server-rendering#readme"
}
